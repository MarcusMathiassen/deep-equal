        "benchee": "^1.1.0",
        "capture-website-cli": "^2.3.0",
        "cli-table3": "^0.6.2",
        "dot": "^1.1.3",
        "fast-deep-equal": "^3.1.3",
        "fast-equals": "^4.0.1",
        "react": "^18.2.0",
        "react-fast-compare": "^3.2.0"
    },
    "files": [
        "dist/index.umd.js",
        "dist/index.esm.js",
        "dist/index.cjs.js",
        "dist/react"
