{
    "name": "@mathi/deep-equal",
    "version": "1.0.0",
    "description": "Deep comparison of objects and arrays",
    "author": "Marcus Mathiassen",
    "license": "MIT",
    "browser": "index.umd.js",
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "exports": {
        ".": "./dist/index.cjs.js",
        "./react": "./dist/react/index.cjs.js"
    },
    "scripts": {
        "test": "jest",
        "benchmark:png": "capture-website ./benchmark/index.html --dark-mode --overwrite --width=1920 --height=1080 --delay=3 --output=benchmark.png",
